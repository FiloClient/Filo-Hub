if game.PlaceId == 698448212 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/FiloClient/Gui-/main/guiv2", true))()
    local Window = Library.CreateLib("Filo Hub", "Sentinel")
    local Main = Window:NewTab("RoBeats")
    local MainSection = Main:NewSection("Scripts")

    -- State variables to track features
    local autoplayEnabled = false
    local autoplayScript
    local espEnabled = false

    local toggleAutoplayButton = MainSection:NewButton("Toggle Autoplay", "Toggle Filo Autoplay on and off", function()
        autoplayEnabled = not autoplayEnabled
        if autoplayEnabled then
            print("Autoplay Enabled")
            autoplayScript = loadstring(game:HttpGet("https://raw.githubusercontent.com/FiloClient/Filo-for-Robeats/main/Autobot", true))()
        else
            print("Autoplay Disabled")
            if autoplayScript and autoplayScript.Stop then
                autoplayScript.Stop()
            end
        end
    end)

    local toggleESPButton = MainSection:NewButton("Toggle ESP", "Toggle Filo ESP on and off", function()
        espEnabled = not espEnabled
        if espEnabled then
            print("ESP Enabled")
            loadstring(game:HttpGet("https://raw.githubusercontent.com/FiloClient/Filo-Hub/main/Esp", true))()
        else
            print("ESP Disabled")
            -- Insert code to disable ESP here
        end
    end)

    -- Player
    local Player = Window:NewTab("Player")
    local PlayerSection = Player:NewSection("Player")

    PlayerSection:NewSlider("SPEEEEED", "This makes you Nagah after stealing", 500, 16, function(s)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
    end)

    PlayerSection:NewSlider("Jump", "This is for my 1.20 classmate named Estripper so he can feel tall", 500, 50, function(s)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
    end)

    -- Other things
    local Others = Window:NewTab("Others")
    local OtherSection = Others:NewSection("Others")

    -- Button to move GUI to bottom right
    OtherSection:NewButton("Move GUI to Bottom Right", "Moves the GUI to the bottom right corner", function()
        local screenSize = game:GetService("Workspace").CurrentCamera.ViewportSize
        local gui = Window.MainFrame
        gui.Position = UDim2.new(1, -300, 1, -150) -- Adjust as necessary for size and position
        gui.Size = UDim2.new(0, 300, 0, 150) -- Adjust as necessary for size and position
    end)

elseif game.PlaceId == 6447798030 then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/FiloClient/fuyk/main/player", true))()

-- For every other game
else
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/FiloClient/Gui-/main/guiv2", true))()
    local Window = Library.CreateLib("Filo Hub", "Sentinel")

    -- Player
    local Player = Window:NewTab("Player")
    local PlayerSection = Player:NewSection("Player")

    PlayerSection:NewSlider("SPEEEEED", "This makes you Nagah after stealing", 500, 16, function(s)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
    end)

    PlayerSection:NewSlider("Jump", "This is for my 1.20 classmate named Estripper so he can jump high", 500, 50, function(s)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
    end)

    -- Other things
    local Others = Window:NewTab("Others")
    local OtherSection = Others:NewSection("Others")

    -- Button to move GUI to bottom right
    OtherSection:NewButton("Move GUI to Bottom Right", "Moves the GUI to the bottom right corner", function()
        local screenSize = game:GetService("Workspace").CurrentCamera.ViewportSize
        local gui = Window.MainFrame
        gui.Position = UDim2.new(1, -300, 1, -150) -- Adjust as necessary for size and position
        gui.Size = UDim2.new(0, 300, 0, 150) -- Adjust as necessary for size and position
    end)
end
